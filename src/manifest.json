{
    "manifest_version": 3,
    "name": "Yapi Codemirror",
    "version": "1.0",
    "description": "Use Codemirror to edit advanced mock scripts of yapi",
    "icons": {
        "16": "./icons/16.png",
        "32": "./icons/32.png",
        "48": "./icons/48.png",
        "128": "./icons/128.png"
    },
    "content_scripts": [
        {
            "matches": ["https://github.com/"],
            "js": ["./content.bundle.js"],
            "css": ["./styles.css"]
        }
    ]
}
